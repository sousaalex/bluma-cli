{
  "mcpServers": {
    "notebook": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "mcp-sequentialthinking-tools"
      ]
    },
    "github": {
        "command": "cmd",
        "args": [
          "/c",
          "npx",
          "-y",
          "@modelcontextprotocol/server-github"
        ],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
        }
      },
    "notion": {
        "command": "cmd",
        "args": [
          "/c",
          "npx",
          "-y",
          "@suekou/mcp-notion-server"
        ],
        "env": {
          "NOTION_API_TOKEN": "${NOTION_API_TOKEN}"
        }
    },
    "upstash_context7-mcp": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "message": {
      "command": "cmd",
      "args": [
       "/c",
        "npx",
        "-y",
        "@sousaalex1605/message_mcp_server"
      ]
    },
    "file_manager": {
      "command": "cmd",
      "args": [
       "/c",
        "npx",
        "-y",
        "@sousaalex1605/file_manager_mcp_server"
      ]
    }
  }
}